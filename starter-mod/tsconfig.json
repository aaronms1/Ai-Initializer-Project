{
  "_version": "9.1",
  "compilerOptions": {
    "sourceMap": true,
    "jsx": "react-jsx",
    "inlineSources": true,
    "module": "esNext",
    "target": "es2020",
    "moduleResolution": "bundler",
    "strict": true,
    "skipLibCheck": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "experimentalDecorators": true,
    "useDefineForClassFields": false,
    "baseUrl": "src/main/frontend",
    "paths": {
      "@vaadin/flow-frontend": [
        "generated/jar-resources"
      ],
      "@vaadin/flow-frontend/*": [
        "generated/jar-resources/*"
      ],
      "Frontend/*": [
        "*"
        ],
    }
  },
  "include": [
    "src/main/frontend/**/*",
    "types.d.ts",
    "admin-mod/src/main/java/org/dacss/projectinitai/admin/**/*",
    "advisers-mod/src/main/java/org/dacss/projectinitai/advisers/**/*",
    "anomalies-mod/src/main/java/org/dacss/projectinitai/anomalies/**/*",
    "checksums-mod/src/main/java/org/dacss/projectinitai/checksums/**/*",
    "classifications-mod/src/main/java/org/dacss/projectinitai/classifications/**/*",
    "clustering-mod/src/main/java/org/dacss/projectinitai/clustering/**/*",
    "databases-mod/src/main/java/org/dacss/projectinitai/databases/**/*",
    "directories-mod/src/main/java/org/dacss/projectinitai/directories/**/*",
    "downloaders-mod/src/main/java/org/dacss/projectinitai/downloaders/**/*",
    "embedding-mod/src/main/java/org/dacss/projectinitai/embedding/**/*",
    "generative-mod/src/main/java/org/dacss/projectinitai/generative/**/*",
    "krr-mod/src/main/java/org/dacss/projectinitai/krr/**/*",
    "loaders-mod/src/main/java/org/dacss/projectinitai/loaders/**/*",
    "messages-mod/src/main/java/org/dacss/projectinitai/messages/**/*",
    "metrics-mod/src/main/java/org/dacss/projectinitai/metrics/**/*",
    "models-mod/src/main/java/org/dacss/projectinitai/models/**/*",
    "nlp-mod/src/main/java/org/dacss/projectinitai/nlp/**/*",
    "optimizations-mod/src/main/java/org/dacss/projectinitai/optimizations/**/*",
    "predictive-mod/src/main/java/org/dacss/projectinitai/predictive/**/*",
    "recognitions-mod/src/main/java/org/dacss/projectinitai/recognitions/**/*",
    "recommendations-mod/src/main/java/org/dacss/projectinitai/recommendations/**/*",
    "reinforcement-mod/src/main/java/org/dacss/projectinitai/reinforcement/**/*",
    "robotics-mod/src/main/java/org/dacss/projectinitai/robotics/**/*",
    "security-mod/src/main/java/org/dacss/projectinitai/security/**/*",
    "servers-mod/src/main/java/org/dacss/projectinitai/servers/**/*",
    "services-mod/src/main/java/org/dacss/projectinitai/services/**/*",
    "snapshots-mod/src/main/java/org/dacss/projectinitai/snapshots/**/*",
    "speech-mod/src/main/java/org/dacss/projectinitai/speech/**/*",
    "starter-mod/src/main/java/org/dacss/projectinitai/starter/**/*",
    "tar-mod/src/main/java/org/dacss/projectinitai/tar/**/*",
    "vision-mod/src/main/java/org/dacss/projectinitai/vision/**/*",
  ],
  "exclude": [
    "src/main/frontend/generated/jar-resources/**"
  ]
}
